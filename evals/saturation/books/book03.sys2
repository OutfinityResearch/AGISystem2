# SAT_QUERY_NEG op=Mentions book=Book03 key=Key_B03_Missing expect=none

@Idea:Idea graph idea concept trait
    isA $idea $concept
    hasProperty $idea $trait
    return $idea
end

@Mentions:Mentions graph book key idea
    @triple __Triple $book $key $idea
    return $triple
end

