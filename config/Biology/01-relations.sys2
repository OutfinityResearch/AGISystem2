# Biology - Domain Relations
# REAL graph definitions for biological relationships

# ===== Metabolic Relations (REAL definitions) =====

@metabolizes:metabolizes graph organism substance
    # Organism breaks down substance for energy/building blocks
    @org __Role Agent $organism
    @sub __Role Theme $substance
    @process __Role Process Metabolism
    @result __Bundle $org $sub $process
    return $result
end

@synthesizes:synthesizes graph organism product precursors
    # Organism builds complex molecule from simpler ones
    @org __Role Agent $organism
    @prod __Role Result $product
    @prec __Role Source $precursors
    @anabolic __Role Type Anabolism
    @result __Bundle $org $prod $prec $anabolic
    return $result
end

@breaksDown:breaksDown graph organism complex simpler
    # Catabolic process: complex â†’ simpler molecules
    @org __Role Agent $organism
    @input __Role Theme $complex
    @output __Role Result $simpler
    @catabolic __Role Type Catabolism
    @result __Bundle $org $input $output $catabolic
    return $result
end

@catalyzes:catalyzes graph enzyme reaction
    # Enzyme speeds up biochemical reaction
    @enz __Role Instrument $enzyme
    @rxn __Role Action $reaction
    @catalysis __Pair $enz $rxn
    return $catalysis
end

# ===== Genetic Relations (REAL definitions) =====

@expresses:expresses graph gene protein
    # Gene codes for protein through transcription/translation
    @g __Role Theme $gene
    @p __Role Result $protein
    @transcription __Role Process Transcription
    @translation __Role Process Translation
    @result __Bundle $g $p $transcription $translation
    return $result
end

@mutatesTo:mutatesTo graph geneOriginal geneVariant
    # Genetic mutation creates variant
    @orig __Role Source $geneOriginal
    @var __Role Result $geneVariant
    @mutation __Role Type Mutation
    @result __Bundle $orig $var $mutation
    return $result
end

@regulatesExpression:regulatesExpression graph regulator targetGene
    # Regulatory element controls gene expression
    @reg __Role Agent $regulator
    @target __Role Target $targetGene
    @control __Pair $reg $target
    return $control
end

# ===== Ecological Relations (REAL definitions) =====

@predatorOf:predatorOf graph predator prey
    # Predator hunts and consumes prey
    @pred __Role Agent $predator
    @prey __Role Theme $prey
    @hunts __Role Action Hunting
    @consumes __Role Action Consumption
    @result __Bundle $pred $prey $hunts $consumes
    return $result
end

@symbiosisWith:symbiosisWith graph organismA organismB benefit
    # Mutual or commensal relationship
    @a __Role Agent $organismA
    @b __Role Agent $organismB
    @benefit __Role Result $benefit
    @symbiosis __Bundle $a $b $benefit
    return $symbiosis
end

@competesFor:competesFor graph organismA organismB resource
    # Competition for limited resource
    @a __Role Agent $organismA
    @b __Role Agent $organismB
    @res __Role Resource $resource
    @competition __Bundle $a $b $res
    return $competition
end

@parasitizes:parasitizes graph parasite host
    # Parasite lives on/in host and harms it
    @par __Role Agent $parasite
    @hos __Role Theme $host
    @harm __Role Result Harm
    @dependency __Role Type Dependency
    @result __Bundle $par $hos $harm $dependency
    return $result
end

# ===== Taxonomic Relations (REAL definitions) =====

@taxonOf:taxonOf graph organism taxon
    # Organism belongs to taxonomic group
    @org __Role Theme $organism
    @tax __Role Category $taxon
    @classification __Pair $org $tax
    return $classification
end

@descendsFrom:descendsFrom graph descendant ancestor
    # Evolutionary descent relationship
    @desc __Role Theme $descendant
    @anc __Role Source $ancestor
    @evolution __Role Process Evolution
    @lineage __Bundle $desc $anc $evolution
    return $lineage
end

# ===== Structural Relations (REAL definitions) =====

@composedOf:composedOf graph whole parts
    # Biological structure made of component parts
    @structure __Role Structure $whole
    @components __Role Component $parts
    @composition __Pair $structure $components
    return $composition
end

# ===== Physiological Relations (REAL definitions) =====

@performs:performs graph organism function
    # Organism carries out biological function
    @org __Role Agent $organism
    @func __Role Function $function
    @capability __Pair $org $func
    return $capability
end

@transports:transports graph system substance fromLocation toLocation
    # Biological transport (circulatory, etc.)
    @sys __Role Agent $system
    @sub __Role Theme $substance
    @from __Role Source $fromLocation
    @to __Role Goal $toLocation
    @transport __Bundle $sys $sub $from $to
    return $transport
end

@secretes:secretes graph organ substance
    # Organ/gland secretes substance
    @org __Role Agent $organ
    @sub __Role Result $substance
    @secretion __Pair $org $sub
    return $secretion
end

# ===== Reproductive Relations (REAL definitions) =====

@reproducesBy:reproducesBy graph organism method
    # Reproduction method (sexual, asexual, etc.)
    @org __Role Agent $organism
    @method __Role Manner $method
    @reproduction __Pair $org $method
    return $reproduction
end

@offspringOf:offspringOf graph child parent
    # Parent-child relationship
    @c __Role Theme $child
    @p __Role Source $parent
    @parenthood __Pair $c $p
    return $parenthood
end

# ===== Causal Relations (REAL definitions) =====

@triggers:triggers graph stimulus response
    # Biological stimulus triggers response
    @stim __Role Cause $stimulus
    @resp __Role Result $response
    @trigger __Pair $stim $resp
    return $trigger
end

# ===== Epidemiology Relations (REAL definitions) =====

@affects:affects graph agent target
    # Agent affects target (e.g., pathogen affects population/location)
    @a __Role Agent $agent
    @t __Role Target $target
    @impact __Pair $a $t
    return $impact
end

@reproductionNumber:reproductionNumber graph pathogen r0
    # Pathogen reproduction number (R0)
    @path __Role Agent $pathogen
    @value __Role Value $r0
    @metric __Pair $path $value
    return $metric
end

@inhibits:inhibits graph inhibitor process
    # Inhibitor reduces/stops process
    @inh __Role Agent $inhibitor
    @proc __Role Process $process
    @inhibition __Pair $inh $proc
    return $inhibition
end

@activates:activates graph activator process
    # Activator initiates/enhances process
    @act __Role Agent $activator
    @proc __Role Process $process
    @activation __Pair $act $proc
    return $activation
end

# ===== FUNDAMENTAL Taxonomy Relations (CORE for Biology) =====

@belongsTo:belongsTo graph entity category
    # Entity belongs to taxonomic/organizational category
    # This is FUNDAMENTAL - used across all taxonomy
    @ent __Role Entity $entity
    @cat __Role Category $category
    @membership __Pair $ent $cat
    return $membership
end

@foundIn:foundIn graph organism location
    # Organism/structure found in location/context
    # This is FUNDAMENTAL - used across ecology, anatomy
    @org __Role Theme $organism
    @loc __Role Location $location
    @presence __Pair $org $loc
    return $presence
end

@hasFunction:hasFunction graph entity function
    # Entity has biological function/role
    # This is FUNDAMENTAL - used across all biological systems
    @ent __Role Entity $entity
    @func __Role Function $function
    @capability __Pair $ent $func
    return $capability
end
