# ===== Logical Relations =====

@supports:supports graph premises conclusion
    # Premises support conclusion
    @prem __Role Premises $premises
    @concl __Role Conclusion $conclusion
    @inference __Role Relation Inference
    @result __Bundle $prem $concl $inference
    return $result
end

@entails:entails graph proposition1 proposition2
    # Proposition logically entails another
    @p1 __Role Antecedent $proposition1
    @p2 __Role Consequent $proposition2
    @entailment __Role Relation LogicalEntailment
    @result __Bundle $p1 $p2 $entailment
    return $result
end

@contradicts:contradicts graph proposition1 proposition2
    # Propositions contradict each other
    @p1 __Role Proposition $proposition1
    @p2 __Role Proposition $proposition2
    @contradiction __Role Relation Contradiction
    @result __Bundle $p1 $p2 $contradiction
    return $result
end

@isValid:isValid graph argument
    # Argument is logically valid
    @arg __Role Argument $argument
    @validity __Role Property Valid
    @formal __Role Domain Logic
    @result __Bundle $arg $validity $formal
    return $result
end

