# Philosophy - Domain Relations
# REAL graph definitions for philosophical relationships

# ===== Metaphysical Relations =====

@exists:exists graph entity
    # Entity has being/existence
    @ent __Role Entity $entity
    @being __Role Property Exists
    @ontological __Role Domain Ontology
    @result __Bundle $ent $being $ontological
    return $result
end

@hasEssence:hasEssence graph entity essence
    # Entity has essential nature
    @ent __Role Entity $entity
    @ess __Role Essence $essence
    @necessary __Role Relation NecessaryProperty
    @result __Bundle $ent $ess $necessary
    return $result
end

@causes:causes graph cause effect
    # Causal relation
    @cau __Role Cause $cause
    @eff __Role Effect $effect
    @causation __Role Relation Causation
    @result __Bundle $cau $eff $causation
    return $result
end

@inheresIn:inheresIn graph property substance
    # Property inheres in substance
    @prop __Role Property $property
    @sub __Role Substance $substance
    @inherence __Role Relation Inherence
    @result __Bundle $prop $sub $inherence
    return $result
end

# ===== Epistemological Relations =====

@knows:knows graph knower proposition
    # Subject knows proposition
    @know __Role Knower $knower
    @prop __Role Proposition $proposition
    @knowledge __Role Relation Knowledge
    @result __Bundle $know $prop $knowledge
    return $result
end

@believes:believes graph believer proposition
    # Subject believes proposition
    @bel __Role Believer $believer
    @prop __Role Proposition $proposition
    @belief __Role Relation Belief
    @result __Bundle $bel $prop $belief
    return $result
end

@justifies:justifies graph evidence belief
    # Evidence justifies belief
    @evid __Role Evidence $evidence
    @bel __Role Belief $belief
    @justification __Role Relation Justification
    @result __Bundle $evid $bel $justification
    return $result
end

@doubts:doubts graph skeptic claim
    # Skeptic doubts claim
    @skep __Role Skeptic $skeptic
    @cl __Role Claim $claim
    @skepticism __Role Attitude Skepticism
    @result __Bundle $skep $cl $skepticism
    return $result
end

@isTrue:isTrue graph proposition
    # Proposition is true
    @prop __Role Proposition $proposition
    @truth __Role Property Truth
    @semantic __Role Domain Semantics
    @result __Bundle $prop $truth $semantic
    return $result
end

